pipeline {
   agent {
       node {
           label 'master'
           customWorkspace 'workspace/VFMHybrid'
       }
   }

   stages {
      stage('npm install packages') {
         steps {
            sh 'nvm use 10'
            sh 'npm i'
         }
      }
   }
   post {
      success {
         echo 'Build Success, Go to download your packages in the status page.'
      }
   }
}
