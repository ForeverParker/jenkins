pipeline {
   agent {
       node {
           label 'master'
           customWorkspace 'workspace/VFMHybrid'
       }
   }

   stages {
      stage('Execute Pre Commands'){
         steps{
             script{
               String commandList = "${PreCommand}";
               echo commandList;
            }
         }
      }
      stage('npm install packages') {
         steps {
            sh 'nvm use 10'
         }
      }
   }
   post {
      success {
         echo 'Build Success, Go to download your packages in the status page.'
      }
   }
}
